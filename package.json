{
	"name": "ottotime",
	"displayName": "ottotime",
	"description": "Track time automatically",
	"version": "1.0.4",
	"author": "Ottomated",
	"publisher": "Ottomated",
	"engines": {
		"vscode": "^1.86.0"
	},
	"license": "GPL-3.0-only",
	"categories": [
		"Other"
	],
	"main": "./dist/extension.js",
	"activationEvents": [
		"onStartupFinished"
	],
	"icon": "icon.png",
	"contributes": {
		"customEditors": [
			{
				"viewType": "ottotime.preview",
				"displayName": "Ottotime",
				"selector": [
					{
						"filenamePattern": ".ottotime"
					}
				]
			}
		],
		"commands": [
			{
				"command": "ottotime.disable",
				"title": "Disable for this workspace",
				"category": "Ottotime"
			},
			{
				"command": "ottotime.enable",
				"title": "Enable for this workspace",
				"category": "Ottotime"
			},
			{
				"command": "ottotime.showtime",
				"title": "Show workspace logs",
				"category": "Ottotime"
			},
			{
				"command": "ottotime.showalltimes",
				"title": "Show all logs",
				"category": "Ottotime"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "ottotime.disable",
					"when": "ottotime.enabled && workbenchState != empty"
				},
				{
					"command": "ottotime.enable",
					"when": "!ottotime.enabled && workbenchState != empty"
				},
				{
					"command": "ottotime.showtime",
					"when": "workbenchState != empty"
				}
			]
		},
		"icons": {
			"otto-ottomated": {
				"description": "Ottomated",
				"default": {
					"fontPath": "icons.woff",
					"fontCharacter": "\\004f"
				}
			}
		},
		"languages": [
			{
				"id": "ottotime",
				"filenames": [
					".ottotime"
				],
				"icon": {
					"dark": "icon-dark.svg",
					"light": "icon-light.svg"
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "pnpm package",
		"compile": "pnpm run check-types && pnpm run lint && bun esbuild.ts",
		"package": "rm -rf dist && pnpm run check-types && pnpm run lint && bun esbuild.ts --production",
		"build": "bun esbuild.ts",
		"check-types": "tsc --noEmit",
		"lint": "eslint src",
		"vsce:package": "vsce package --no-dependencies -o ottotime.vsix"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ottomated/ottotime"
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@types/bun": "^1.1.14",
		"@types/node": "~22.10.2",
		"@types/vscode": "1.86.0",
		"@vscode/vsce": "^3.2.1",
		"date-fns": "^4.1.0",
		"esbuild": "^0.24.2",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"globals": "^15.14.0",
		"lightningcss": "^1.28.2",
		"prettier": "^3.4.2",
		"svelte": "^5.15.0",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.18.1"
	},
	"dependencies": {
		"devalue": "^5.1.1",
		"nanostores": "^0.11.3"
	},
	"packageManager": "pnpm@9.15.1"
}
