{
	"name": "ottotime",
	"displayName": "ottotime",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.78.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"extensionDependencies": [
		"vscode.git"
	],
	"contributes": {
		"commands": [
			{
				"command": "ottotime.showtime",
				"title": "Show Project Hours",
				"category": "Ottotime"
			},
			{
				"command": "ottotime.setApiKey",
				"title": "Set API Key",
				"category": "Ottotime"
			},
			{
				"command": "ottotime.setApiUrl",
				"title": "Set API URL",
				"category": "Ottotime"
			},
			{
				"command": "ottotime.setTimeout",
				"title": "Set Idle Timeout",
				"category": "Ottotime"
			}
		],
		"icons": {
			"otto-ottomated": {
				"description": "Ottomated",
				"default": {
					"fontPath": "icons.woff",
					"fontCharacter": "\\004f"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "tsup",
		"watch": "tsup --watch",
		"lint": "eslint src --fix"
	},
	"devDependencies": {
		"@types/git-url-parse": "^9.0.1",
		"@types/node": "20.2.5",
		"@types/vscode": "^1.78.0",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"chart.js": "^4.3.0",
		"date-fns": "^2.30.0",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"git-url-parse": "^13.1.0",
		"prettier": "^2.8.8",
		"tsup": "^6.7.0",
		"typescript": "^5.1.3",
		"undici": "^5.22.1"
	}
}
